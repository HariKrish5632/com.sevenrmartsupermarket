<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="7mart Cross Browser Testing" thread-count="10" parallel="tests">
	<test name="RunningInChrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
	<test name="RunningInEdge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
	<test name="RunningInFiefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
</suite>

